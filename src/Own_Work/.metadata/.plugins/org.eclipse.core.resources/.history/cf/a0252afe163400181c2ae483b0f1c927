package com.sunbeaminfo.inheritance;

public class Address {
	private String line1;
	private String line2;
	private String city;
	private String pin;
	
	public Address()
	{
		this("","","","");
	}
	public Address(String line1, String line2, String city, String pin) {
		super();
		this.line1 = line1;
		this.line2 = line2;
		this.city = city;
		this.pin = pin;
	}
	
	
}
